- if @exchanges.present?
  %center
    %h2 Exchange rates
    %table{:border => 1}
      %tbody
        %tr
          %td Creation date
          %td Actions
        - @exchanges.each do |exchange|
          %tr
            %td= exchange.name
            %td= link_to 'Show', money_path(exchange)

    = will_paginate @exchanges